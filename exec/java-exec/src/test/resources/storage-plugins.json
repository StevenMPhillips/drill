{
  "storage":{
    dfs: {
      type: "file",
      connection: "file:///",
      workspaces: {
        "home" : {
          location: "/",
          writable: false
        },
        "tmp" : {
          location: "/tmp/drilltest",
          writable: true,
          storageformat: "csv"
        },
        "tpch" : {
          location: "/Users/sphillips/tpch_SF1/",
          writable: false
        }
      },
      formats: {
            "psv" : {
              type: "text",
              extensions: [ "tbl" ],
              delimiter: "|"
            },
            "csv" : {
              type: "text",
              extensions: [ "csv", "bcp" ],
              delimiter: ","
            },
            "tsv" : {
              type: "text",
              extensions: [ "tsv" ],
              delimiter: "\t"
            },
            "json" : {
              type: "json"
            },
            "parquet" : {
              type: "parquet"
            }
          }
    },
    cp: {
      type: "file",
      connection: "classpath:///",
      formats: {
        "json" : {
          type: "json"
        },
        "parquet" : {
          type: "parquet"
        }
      }
    }
  }
}
